{
  "name": "check-if-windows",
  "version": "0.0.0",
  "description": "Returns a boolean indicating if the current process is running on Windows.",
  "author": {
    "name": "Athan Reines",
    "email": "kgryte@gmail.com"
  },
  "contributors": [
    {
      "name": "Athan Reines",
      "email": "kgryte@gmail.com"
    }
  ],
  "scripts": {
    "test": "mocha",
    "test-cov": "istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "codecov": "istanbul cover ./node_modules/.bin/_mocha --dir ./reports/codecov/coverage --report lcovonly -- -R spec && cat ./reports/codecov/coverage/lcov.info | codecov && rm -rf ./reports/codecov"
  },
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git://github.com/kgryte/node-check-if-windows.git"
  },
  "keywords": [
    "utils",
    "utilities",
    "utility",
    "process",
    "platform",
    "os",
    "check",
    "windows",
    "posix",
    "linux",
    "darwin",
    "is",
    "iswindows",
    "win32"
  ],
  "bugs": {
    "url": "https://github.com/kgryte/node-check-if-windows/issues"
  },
  "dependencies": {
    "utils-platform": "^1.0.0"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "codecov.io": "^0.1.5",
    "istanbul": "^0.3.0",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x",
    "mocha": "2.x.x",
    "proxyquire": "^1.7.2"
  },
  "license": "MIT"
}
